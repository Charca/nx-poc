{
  "npmScope": "helpscout-js",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "dashboard": {
      "tags": []
    },
    "dashboard-e2e": {
      "tags": [],
      "implicitDependencies": ["dashboard"]
    },
    "mailbox": {
      "tags": []
    },
    "mailbox-e2e": {
      "tags": [],
      "implicitDependencies": ["mailbox"]
    },
    "reports": {
      "tags": []
    },
    "reports-e2e": {
      "tags": [],
      "implicitDependencies": ["reports"]
    },
    "common-utils": {
      "tags": ["shared"]
    },
    "common-constants": {
      "tags": []
    },
    "hsds": {
      "tags": []
    },
    "common-components": {
      "tags": []
    },
    "docs": {
      "tags": ["legacy"]
    },
    "docs-e2e": {
      "tags": [],
      "implicitDependencies": ["docs"]
    },
    "common-legacy": {
      "tags": ["legacy"]
    }
  }
}
